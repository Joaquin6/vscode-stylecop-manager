  "icon": "images/icon.png",
  "bugs": {
    "url": "https://github.com/Joaquin6/vscode-stylecop-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joaquin6/vscode-stylecop-manager.git"
  },
  "engines": {
    "vscode": "^1.40.0"
